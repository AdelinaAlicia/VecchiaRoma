<!DOCTYPE html>
<html lang="en">
  <head>
    {{>head}}
  </head>
  <body>
    {{>nav}}

    <section class="ftco-section">
    <div class="container">
    	<div class="row justify-content-center mb-5 pb-3">
          <div class="col-md-11 text-center heading-section ftco-animate">

           <div class="row">

        	


    <div class="heading-menu text-center ftco-animate">
        			<h3>Lunch</h3>
        		</div>
				{{#Lunch}}
					<div class="menus d-flex ftco-animate">
							<div class="menu-img img" style="background-image: url({{image}});"></div>
							<div class="text">
								<div class="d-flex">
									<div class="one-half">
									<h3>{{name}}</h3>
									</div>
									<div class="one-forth">
									<span class="price">{{price}}ron</span>
									</div>
								</div>
								<p><span>{{description}}</span></p>
							</div>
							</div>
				{{/Lunch}}
        	</div>
    </section>

    </div>

  </body>
</html>